{
  "kind": "collectionType",
  "collectionName": "usuarios",
  "info": {
    "singularName": "usuario",
    "pluralName": "usuarios",
    "displayName": "Usuarios"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "nome": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "email",
      "required": true
    },
    "usuario": {
      "type": "string"
    },
    "senha": {
      "type": "password",
      "required": true,
      "private": true
    },
    "artigos": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::artigo.artigo",
      "mappedBy": "usuario"
    }
  }
}
