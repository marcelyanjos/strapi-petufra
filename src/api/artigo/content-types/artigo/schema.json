{
  "kind": "collectionType",
  "collectionName": "artigos",
  "info": {
    "singularName": "artigo",
    "pluralName": "artigos",
    "displayName": "Artigos",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "titulo": {
      "type": "string",
      "required": true
    },
    "data_pub": {
      "type": "datetime"
    },
    "data_edit": {
      "type": "datetime"
    },
    "descricao": {
      "type": "text"
    },
    "ilustracao": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "não publicado",
        "publicado"
      ],
      "default": "não publicado"
    },
    "usuario": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::usuario.usuario",
      "inversedBy": "artigos"
    },
    "body": {
      "type": "customField",
      "options": {
        "output": "HTML",
        "preset": "rich"
      },
      "customField": "plugin::ckeditor.CKEditor"
    },
    "categorias": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::categoria.categoria",
      "mappedBy": "artigo"
    }
  }
}
