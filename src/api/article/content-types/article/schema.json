{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "singularName": "article",
    "pluralName": "articles",
    "displayName": "Article",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "Title"
    },
    "Image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ]
    },
    "Published": {
      "type": "enumeration",
      "enum": [
        "Rascunho",
        "Em revisão",
        "Alterações solicitadas",
        "Publicação agendada",
        "Publicados"
      ],
      "default": "Rascunho"
    },
    "categoria": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::categoria.categoria",
      "inversedBy": "articles"
    },
    "publicationStages": {
      "type": "json"
    },
    "autor": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "admin::user"
    },
    "publishedDate": {
      "type": "datetime"
    },
    "content": {
      "type": "customField",
      "options": {
        "output": "HTML",
        "preset": "rich"
      },
      "required": true,
      "customField": "plugin::ckeditor.CKEditor"
    }
  }
}
